#include <iostream>

int main() {
    char alpha;
    int ch;
    std::cout << "Enter the character: ";
    std::cin>>alpha;
    ch=(int)alpha;
    std::cout<<alpha<<"is"<<ch;
    if(ch>=65&&ch<=90){
        std::cout<<"\nUpper Case";
    }
    else if(ch>=97&&ch<=122){
        std::cout<<"\nLower Case";
    }
    else
        std::cout<<"Invalid Character";

    return 0;
}
